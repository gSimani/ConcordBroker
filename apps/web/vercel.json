{
  "version": 2,
  "projectId": "prj_l6jgk7483iwPCcaYarq7sMgt2m7L",
  "orgId": "team_OIAQ7q0bQTblRPZrnKhU4BGF",
  "framework": "vite",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://concordbroker-railway-production.up.railway.app/api/$1",
      "headers": {
        "x-forwarded-host": "$host"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "https://concordbroker-railway-production.up.railway.app/health"
    }
  ],
  "env": {
    "VITE_VERCEL_ENV": "@vercel_env",
    "VITE_VERCEL_URL": "@vercel_url"
  }
}